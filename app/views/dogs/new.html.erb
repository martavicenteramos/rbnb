<h1>Add your puperity!</h1>

<div class="container">
  <div class="row">
    <div class="col" id="edit">
      <%= simple_form_for @dog do |f| %>
      <%= f.input :name%>
      <%= f.input :description%>
      <%= f.input :size,  collection: [ "Small", "Medium", "Large"]%>
      <%= f.input :breed%>
      <%= f.input :gender, collection: [ "Male", "Female"]%>
      <%= f.input :age%>
      <%= f.input :location%>
      <%= f.button :submit, "Update", class:"btn btn-primary" %>
      <% end %>
    </div>
</div>


