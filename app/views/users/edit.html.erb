<h2 class="maintitle">Edit profile</h2>


<div class="container">
  <div class="row">
    <div class="col generic-form" id="edit">
      <%= simple_form_for @user do |f| %>
      <%= f.input :first_name%>
      <%= f.input :last_name%>
      <%= f.input :email %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.button :submit, "Update", class: "btn-generic" %>
      <% end %>
    </div>
    <!--  -->
  </div>
  <br>
  <div class="row">
    <%= link_to 'Go back', user_path, class: "btn-generic pad-cont center"%>
  </div>
</div>
